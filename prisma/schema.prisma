generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Flight {
  id             String   @id @default(cuid())
  airline        String
  source         String
  destination    String
  depTime        String
  arrivalTime    String
  duration       String
  totalStops     Int
  additionalInfo String?
  price          String
  date           String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  route          String
}

model User {
  id        String   @id @default(cuid())
  name      String 
}